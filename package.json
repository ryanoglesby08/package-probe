{
  "name": "package-probe",
  "version": "0.1.0",
  "description": "Scan a Github organization for usage of a package",
  "keywords": [
    "Github",
    "scan",
    "crawl"
  ],
  "homepage": "https://github.com/ryanoglesby08/package-probe#readme",
  "bugs": {
    "url": "https://github.com/ryanoglesby08/package-probe/issues"
  },
  "license": "ISC",
  "author": {
    "name": "Ryan Oglesby",
    "url": "http://ryanogles.by/"
  },
  "repository": "github:ryanoglesby08/package-probe",
  "main": "./bin/probe.js",
  "bin": "./bin/cli.js",
  "files": [
    "bin"
  ],
  "scripts": {
    "build": "rm -rf bin && babel ./src -d bin --ignore __mocks__",
    "build:watch": "babel ./src -d bin --watch",
    "commit": "git-cz",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@octokit/rest": "^15.2.5",
    "babel-runtime": "^6.26.0",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "lodash": "^4.17.5",
    "node-fetch": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.1.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook",
      "pre-commit": "pretty-quick --staged && npm run build && npm run test"
    }
  }
}
