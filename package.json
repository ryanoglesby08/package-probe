{
  "name": "package-probe",
  "version": "0.0.0-development",
  "description": "Scan a Github organization for usage of a package",
  "keywords": [
    "Github",
    "scan",
    "crawl"
  ],
  "homepage": "https://github.com/ryanoglesby08/package-probe#readme",
  "bugs": {
    "url": "https://github.com/ryanoglesby08/package-probe/issues"
  },
  "license": "ISC",
  "author": {
    "name": "Ryan Oglesby",
    "url": "http://ryanogles.by/"
  },
  "repository": "github:ryanoglesby08/package-probe",
  "main": "./bin/probe.js",
  "bin": "./bin/package-probe.js",
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "build": "rm -rf bin && tsc",
    "build:watch": "tsc --watch",
    "commit": "git-cz",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "axios": "^0.27.2",
    "cli-table": "^0.3.11",
    "commander": "^9.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@pollyjs/adapter-node-http": "^6.0.5",
    "@pollyjs/core": "^6.0.5",
    "@pollyjs/persister-fs": "^6.0.5",
    "@types/cli-table": "^0.3.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "babel-jest": "^28.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.2",
    "setup-polly-jest": "^0.10.0",
    "typescript": "^4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
